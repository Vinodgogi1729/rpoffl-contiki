      <skin>se.sics.cooja.plugins.skins.IDVisualizerSkin</skin>
      <viewport>11.275394102671033 0.0 0.0 11.275394102671033 -938.1049496943455 -866.2026211709137</viewport>
    </plugin_config>
    <width>300</width>
    <z>3</z>
    <height>300</height>
    <location_x>980</location_x>
    <location_y>0</location_y>
  </plugin>
  <plugin>
    se.sics.cooja.plugins.LogListener
    <plugin_config>
      <filter />
