    <width>1280</width>
    <z>1</z>
    <height>460</height>
    <location_x>0</location_x>
    <location_y>315</location_y>
  </plugin>
  <plugin>
    se.sics.cooja.plugins.TimeLine
    <plugin_config>
      <mote>0</mote>
